{:des "SpaceFn" :rules [
  [:spacebar ["SpcFn" 1] nil {:afterup ["SpcFn" 0] :alone :spacebar}]
  [:##h                   :##left_arrow        ["SpcFn" 1]]
  [:##j                   :##down_arrow        ["SpcFn" 1]]
  [:##k                   :##up_arrow          ["SpcFn" 1]]
  [:##l                   :##right_arrow       ["SpcFn" 1]]
  [:semicolon             :delete_or_backspace ["SpcFn" 1]]
  [:b                     :escape              ["SpcFn" 1]]
  [:n                     :return_or_enter     ["SpcFn" 1]]
  [:m                     :page_down           ["SpcFn" 1]]
  [:comma                 :page_up             ["SpcFn" 1]]
  [:period                :!Cperiod            ["SpcFn" 1]]
  [:slash                 :!Cslash             ["SpcFn" 1]]
  [:#Sdelete_or_backspace :#Sdelete_forward    ["SpcFn" 1]]
  [:#Stab                 :!C#Stab             ["SpcFn" 1]]
  [:#Sw                   :!T#Stab             ["SpcFn" 1]]
  [:#Ss                   :!C#Ss               ["SpcFn" 1]]
  [:#Sx                   :!C#Sx               ["SpcFn" 1]]
  [:#Sc                   :!C#Sc               ["SpcFn" 1]]
  [:#Sv                   :!C#Sv               ["SpcFn" 1]]
]}

{:des "FJFn" :rules
