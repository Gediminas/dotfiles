  [:l :!Sl :FFn]
  [:semicolon        :!Ssemicolon        :FFn]
  [:quote            :!Squote            :FFn]
  [:non_us_backslash :!Snon_us_backslash :FFn]

  [:grave_accent_and_tilde :!Sgrave_accent_and_tilde :JFn]
  [:z :!Sz :JFn]
  [:x :!Sx :JFn]
  [:c :!Sc :JFn]
  [:v :!Sv :JFn]
  [:b :!Sb :JFn]
  [:b :!Sb :FFn]
  [:n :!Sn :FFn]
  [:m :!Sm :FFn]
  [:comma  :!Scomma  :FFn]
  [:period :!Speriod :FFn]
  [:slash  :!Sslash  :FFn]
]}

